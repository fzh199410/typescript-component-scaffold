{
  "name": "typescript-component-scaffold",
  "version": "1.0.2",
  "description": "scaffold of common component enhanced with typescript",
  "license": "ISC",
  "browser": "dist/index.js",
  "scripts": {
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js --colors --display-error-details",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.babel.js --colors",
    "mock": "node mock-server/server.js",
    "start:mock": "concurrently \"npm start\" \"npm run mock\"",
    "lint:js": "eslint -c .eslintrc.prod.json --fix",
    "test": "jest",
    "package": "cross-env BUILD_PATH=dist npm run build:umd",
    "publish:patch": "npm run package && gulp version-patch && npm publish",
    "publish:minor": "npm run package && gulp version-minor && npm publish",
    "publish:major": "npm run package && gulp version-major && npm publish",
    "unpublish": "npm unpublish -f",
    "git:push": "gulp git-push"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint:js",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^3.26.12",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "echarts": "^3.8.5",
    "eslint": "^5.6.0",
    "eslint-loader": "^2.1.0",
    "event-stream": "^4.0.1",
    "file-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-bump": "^3.1.1",
    "gulp-cli": "^2.0.1",
    "gulp-css-base64": "^1.3.4",
    "gulp-less": "^4.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "gulp-zip": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "json-server": "^0.14.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "peerDependencies": {
    "antd": ">=3.26.12",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "devServer": {
    "local": {
      "port": 8888
    },
    "mock": {
      "port": 3001
    },
    "api": {
      "host": "http://localhost",
      "port": 3001
    }
  },
  "libraryName": "pangoo-mro-component"
}
